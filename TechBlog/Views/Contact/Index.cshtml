<script type="text/javascript">
    function submitContact() {
        var data = $("#concatcForm").serialize();
        $.ajax({
            type: 'POST',
            url: '/Contact/Index',
            contentType: 'application/x-www-form-urlencoded; charset=UTF-8',
            data: data,
